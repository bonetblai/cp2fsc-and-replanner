        (pit-at ?p - pos)
        (wumpus-at ?p - pos)
        (gold-at ?p - pos)
        (got-the-treasure)
        (safe ?p - pos) 
        (stench ?p - pos)
        (breeze ?p - pos)
        (glitter ?p - pos)
    )

