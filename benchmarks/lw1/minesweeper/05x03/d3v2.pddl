    (:var-group (vgroup ?p - pos) (forall (?q - pos) such-that (neighborhood ?p ?q) (mine-var ?q)))
    (:var-group (vxx) (first-movement))

    (:action start-action
        :precondition (need-start)
        :effect (and (not (need-start)) (need-first-move))
        :sensing
            (forall (?p - pos) (model-for obs-for-start-action (start-obs ?p) (first-move ?p)))
    )
    (:action make-first-move
        :parameters (?p - pos)
        :precondition (and (first-move ?p) (need-first-move))
        :effect (not (need-first-move))
        :sensing
            (model-for (first-obs-var ?p) (first-obs ?p) (not (mine-at ?p)))
            (model-for (first-obs-var ?p) (not (first-obs ?p)) (mine-at ?p))
    )

